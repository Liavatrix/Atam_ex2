


main:	br assign
assign:	mov dict, r1
		mov strings, r2
		br check_if_match

check_if_match:		  br check_exactly
second_check:		  inc r1
scnd_check_loop:	  cmpb (r2), #'@
					  beq success_check_exactly
					  cmpb (r1), #'@
					  beq end_word
					  cmpb (r1), (r2)
					  bne reset_loop_1
					  inc r1
					  inc r2
					  inc r4
					  br scnd_check_loop
reset_loop_1:	dec r1
				dec r2
				dec r4
				cmp r4, #0
				bne reset_loop_1
third_check:	      dec r1
					  inc r2
thrd_check_loop:	  cmpb (r2), #'@
					  beq success_check_exactly
					  cmpb (r1), #'@
					  beq end_word
					  cmpb (r1), (r2)
					  bne rst_loop_2
					  inc r1
					  inc r2
					  inc r4
					  br thrd_check_loop
rst_loop_2:	dec r1
				dec r2
				dec r4
				cmp r4, #0
				bne rst_loop_2	
fourth_check:		  inc r1
frth_check_loop:	  cmpb (r2), #'@
					  beq success_check_exactly
					  cmpb (r1), #'@
					  beq end_word
					  cmpb (r1), (r2)
					  bne loop_3
					  inc r1
					  inc r2
					  inc r4
					  br frth_check_loop
loop_3:	dec r1
				dec r2
				dec r4
				cmp r4, #0
				bne loop_3				

		beq end_preffix
		cmpb (r2), #'@
		beq end_preffix
		cmpb (r1), (r2)
		bne main
		inc r1
		inc r2
		br check_if_match

check_exactly: cmpb (r2), #'@
			   beq success_check_exactly
			   cmpb (r1), #'@
			   br end_word
			   cmpb (r1), (r2)
			   bne second_check
			   inc r1
			   inc r2
			   br check_exactly
			   
success_check_exactly: br main
;----------------------------
string_len: .word 2
strings: .word str1
		 .word str2
str1: .ascii<atam@>
str2: .ascii<combi@>
.even
dict_len: .word 5
dict: .word term1
	  .word term2
	  .word term3
	  .word term4
	  .word term5
term1: .ascii <atam@>
term2: .ascii <azam@>
term3: .ascii <atmosphere@>
term4: .ascii <matam@>
term5: .ascii <combi@>
;----------------------------
